{
  "name": "Forward",
  "plural": "forwards",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "dateCreated": {
      "type": "date",
      "required": true
    },
    "dateOfForward": {
      "type": "date",
      "required": true
    },
    "dateReveal": {
      "type": "date",
      "required": true
    },
    "revealInterval": {
      "type": "string",
      "required": true
    },
    "isRemo": {
      "type": "boolean",
      "required": true
    }
  },
  "validations": [],

  "relations": {
    "toUsers": {
      "type": "referencesMany",
      "model": "HeymoUser",
      "foreignKey": "toUserIds",
      "options": {
        "validate": false,
        "forceId": false
      }
    }
  },

  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
