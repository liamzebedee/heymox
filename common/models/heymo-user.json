{
  "name": "HeymoUser",
  "plural": "users",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "username": {
      "type": "string",
      "required": true
    },
    "password": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "contacts": {
      "type": "hasMany",
      "model": "HeymoUser",
      "foreignKey": "userId",
      "through": "Contact",
      "keyThrough": "contactId"
    },
    "createdMoments": {
      "type": "hasMany",
      "model": "Moment",
      "foreignKey": "creatorUserId",
      "options": {
        "validate": false
      }
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "contacts"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
